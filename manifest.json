{
  "manifest_version": 3,
  "name": "Auto Data Cleaner",
  "version": "1.0",
  "description": "Auto clears browsing data.",
  "icons": {
    "16": "img/shield16.png",
    "32": "img/shield32.png",
    "48": "img/shield48.png",
    "128": "img/shield128.png"
  },
  "permissions": [
    "browsingData",
    "storage",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Auto Cleaner",
    "default_popup": "popup.html"
  }
}