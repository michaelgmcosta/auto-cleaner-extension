{
  "manifest_version": 3,
  "name": "Auto Data Cleaner",
  "version": "1.0",
  "description": "Clears browsing data every day at the end of the day.",
  "icons": {
    "16": "img/webpage16.png",
    "32": "img/webpage32.png",
    "48": "img/webpage48.png",
    "128": "img/webpage128.png"
  },
  "permissions": [
    "browsingData",
    "storage",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Auto Cleaner"
  }
}